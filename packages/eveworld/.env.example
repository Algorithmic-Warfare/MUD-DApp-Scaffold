# Admin configuration
EVE_TOKEN_ADMIN=YOUR_ADMIN_ADDRESS_HERE
ADMIN_ACCOUNTS=COMMA_SEPARATED_ADMIN_ADDRESSES

# Token configuration
EVE_TOKEN_NAME="EVE Token"
EVE_TOKEN_SYMBOL="EVE"